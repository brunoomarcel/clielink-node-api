// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  createdAt DateTime @default(now())
}

model Url {
  idUrl       String   @id @default(uuid())
  originalUrl String
  shortCode   String
  createdAt   DateTime @default(now())
  clicks      Int      @default(0)
}